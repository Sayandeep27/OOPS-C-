#include <bits/stdc++.h>
using namespace std;


class Sum{
    
  public:   // (Compile time polymorphism) Function overloading - where same function names but different parameters
  
     void add(int x,int y)
     {
         int sum=x+y;
         
         cout<<sum<<endl;
     }
     
     
     
     void add(int x,int y,int z)
     {
         int sum=x+y+z;
         
         cout<<sum<<endl;
     }
     
     
     
     void add(float x,float y)
     {
         float sum=x+y;
         
         cout<<sum<<endl;
     }
    
    
};



int main()
{
    
    Sum s;
    
    s.add(2,3);
    
    s.add(1,4,5);
    
    s.add(float(2.3),float(2.7));
    
    
    
    

    return 0;
}
